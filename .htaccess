        Options -Indexes
        RewriteEngine On 
        RewriteCond %{REQUEST_URI} !^/simpapp_public/ 
        RewriteCond %{REQUEST_FILENAME} !-f 
        RewriteCond %{REQUEST_FILENAME} !-d 
        RewriteRule ^(.*)$ simpapp_public/ 
        RewriteRule ^(/)?$ simpapp_public/index.php [L]
    
